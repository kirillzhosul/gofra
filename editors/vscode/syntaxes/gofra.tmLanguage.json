{
    "scopeName": "source.gofra",
    "patterns": [
        {
            "name": "keyword.control.gofra",
            "match": "\\b(if|while|end|extern|inline|func|do|var|typecast|return)\\b"
        },
        {
            "name": "keyword.preprocessor.gofra",
            "match": "(#macro|#define|#include|#endif|#ifdef)"
        },
        {
            "name": "keyword.operator.gofra",
            "match": "(==|!=|<=|>=|<|>)"
        },
        {
            "name": "string.quoted.double.gofra",
            "begin": "\"",
            "end": "\"",
            "patterns": [
                {
                    "name": "constant.character.escape.gofra",
                    "match": "\\\\."
                }
            ]
        },
        {
            "name": "keyword.other.gofra",
            "match": "\\b(drop|call|copy|syscall0|syscall1|syscall2|syscall3|syscall4|syscall5|syscall6|inc|dec)\\b"
        },
        {
            "name": "comment.line.gofra",
            "match": "//.*$"
        },
        {
            "name": "punctuation.definition.parameters.begin.gofra",
            "match": "\\["
        },
        {
            "name": "punctuation.definition.parameters.end.gofra",
            "match": "\\]"
        },
        {
            "name": "punctuation.separator.parameter.gofra",
            "match": ","
        },
        {
            "name": "storage.type.gofra",
            "match": "\\b(int|ptr|void|bool|char)\\b"
        }
    ]
}